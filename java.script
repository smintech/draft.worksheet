document.getElementById("changeTextBtn").onclick = function() {
    document.getElementById("title").innerText = "Text Changed!";
}
document.getElementById("changeColorBtn").onclick = function() {
    document.body.style.backgroundColor = "#f0c000";
}
let colors = ["red", "blue", "green", "orange", "purple"];
let index = 0;

setInterval(function() {
    document.body.style.backgroundColor = colors[index];

    index++;
    if (index === colors.length) {
        index = 0;
    }
}, 1000);